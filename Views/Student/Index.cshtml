@{
    ViewData["Title"] = "Student Page";
}

@model List<asp_net_mvc.Models.Student>

<div><a class="btn btn-md btn-primary" asp-area="" asp-controller="Student" asp-action="Create">Add Student</a></div>

<h2>Student List</h2>

<table class="table table-striped table-hover">
    <thead>
        <tr><th>Name</th><th>Age</th></tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Name</td>
                <td>@student.Age</td>
            </tr>
        }
    </tbody>
</table>